#! /usr/bin/env expect

set timeout -1

spawn qemu-system-x86_64 \
    -no-kvm \
    -machine pc \
    -cpu qemu64 \
    -accel tcg \
    -drive file=qemu/zbox.img,format=raw \
    -nographic

expect -ex {zbox login: }
send "root\n"
expect -ex {zbox:~# }
send "dd if=/dev/urandom of=data bs=1M count=16\n"
expect -ex {zbox:~# }
